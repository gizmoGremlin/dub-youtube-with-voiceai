{
  "name": "dub-youtube-with-voiceai",
  "version": "0.1.0",
  "description": "Dub YouTube videos with Voice.ai TTS â€” turn scripts into publish-ready voiceovers with chapters, captions, and audio replacement.",
  "type": "module",
  "bin": {
    "voiceai-vo": "./dist/cli.js"
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "start": "node dist/cli.js",
    "bundle": "esbuild src/cli.ts --bundle --platform=node --target=node20 --format=cjs --outfile=voiceai-vo.cjs",
    "test": "vitest run",
    "test:watch": "vitest",
    "lint": "eslint src/",
    "format": "prettier --write \"src/**/*.ts\""
  },
  "engines": {
    "node": ">=20"
  },
  "keywords": [
    "voice.ai",
    "tts",
    "voiceover",
    "youtube",
    "dubbing",
    "chapters",
    "captions",
    "ffmpeg",
    "text-to-speech",
    "agent-skill"
  ],
  "author": "Voice.ai",
  "license": "MIT",
  "dependencies": {
    "chalk": "^5.3.0",
    "commander": "^12.1.0",
    "dotenv": "^16.4.7"
  },
  "devDependencies": {
    "@types/node": "^20.17.0",
    "esbuild": "^0.27.3",
    "eslint": "^9.14.0",
    "prettier": "^3.4.0",
    "typescript": "^5.6.0",
    "typescript-eslint": "^8.13.0",
    "vitest": "^2.1.0"
  }
}
